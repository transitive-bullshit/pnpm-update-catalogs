{
  "name": "pnpm-update-catalogs",
  "version": "1.0.3",
  "description": "pnpm update for pnpm workspace catalogs.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transitive-bullshit/pnpm-update-catalogs.git"
  },
  "packageManager": "pnpm@10.6.4",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "bin": {
    "pnpm-update-catalogs": "./dist/index.js"
  },
  "source": "./bin/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "del clean",
    "test": "run-s test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prebuild": "run-s clean",
    "preinstall": "npx only-allow pnpm",
    "prerelease": "run-s build",
    "pretest": "run-s build"
  },
  "dependencies": {
    "@pnpm/cli-meta": "^1000.0.4",
    "@pnpm/client": "^1000.0.12",
    "@pnpm/config": "^1002.5.4",
    "@pnpm/pick-registry-for-package": "^1000.0.4",
    "cleye": "^1.3.4",
    "exit-hook": "^4.0.0",
    "ora": "^8.2.0",
    "p-map": "^7.0.3",
    "plur": "^5.1.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.13.13",
    "bumpp": "^10.1.0",
    "del-cli": "^6.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "npm-run-all2": "^7.0.2",
    "only-allow": "^1.2.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "3.0.9"
  }
}
